{
  "name": "soundworks",
  "version": "2.2.4",
  "description": "Full stack Javascript framework to create collective audio visual experiences based on mobile devices",
  "authors": [
    "Benjamin Matuszewski",
    "SÃ©bastien Robaszkiewicz",
