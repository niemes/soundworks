  "main": "",
  "scripts": {
    "deploy": "np --yolo",
    "doc": "jsdoc -c jsdoc.json",
    "transpile": "node ./bin/runner --transpile",
    "prewatch": "npm run transpile",
    "version": "npm run transpile && npm run doc && git add docs",
    "watch": "node ./bin/runner --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/collective-soundworks/soundworks"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true
    },
    "rules": {
      "quotes": [
        1,
        "single"
      ],
      "no-underscore-dangle": 0
    }
  },
  "dependencies": {
    "@ircam/basic-controllers": "^1.0.4",
